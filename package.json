{
  "name": "psp-backend",
  "version": "1.0.0",
  "description": "rest node es6",
  "main": "server.ts",
  "scripts": {
    "start": "npm run build:live",
    "start_test": "set NODE_ENV=test && npm run unit:test",
    "start_dev": "set NODE_ENV=development && npm run build:live",
    "start_prod": "set NODE_ENV=production && npm run build:live",
    "start_dev_ops": "set NODE_ENV=devops && npm run build:liveWatch",
    "start_test_vagrant": "export NODE_ENV=test && npm run build:live",
    "start_dev_vagrant": "export NODE_ENV=development && npm run build:live",
    "start_prod_vagrant": "export NODE_ENV=production && npm run build:live",
    "start_dev_ops_vagrant": "export NODE_ENV=devops && npm run build:liveWatch",
    "build:clean": "rm -r ./dist/*",
    "build:live": " nodemon -L --exec ./node_modules/.bin/ts-node -- ./src/server.ts ",
    "build:liveWatch": " nodemon --watch ./node_modules/.bin/ts-node -- ./src/server.ts ",
    "nodemon": "nodemon",
    "unit:test": " mocha --opts ./src/tests/unit/config/mocha.opts ./src/tests/unit/unit.test.js",
    "debug": "node --nolazy --inspect-brk=9229 server.js"
  },
  "keywords": [
    "API",
    "REST",
    "NODE",
    "Typescript"
  ],
  "author": "Cleibson Gomes da Silva",
  "license": "ISC",
  "dependencies": {
    "body-parser": "1.18.1",
    "cors": "2.8.4",
    "crypto-js": "3.1.9-1",
    "date-fns": "1.29.0",
    "express": "4.15.4",
    "express-validator": "4.3.0",
    "glob": "7.1.2",
    "http-status": "1.0.1",
    "jsonwebtoken": "8.0.1",
    "later": "1.2.0",
    "lodash": "4.17.4",
    "morgan": "1.8.2",
    "mysql2": "1.5.0",
    "node-calendar": "0.1.4",
    "path": "0.12.7",
    "reflect-metadata": "0.1.10",
    "sequelize": "5.21.1",
    "sequelize-typescript": "0.6.0",
    "shuffle-array": "1.0.1",
    "socket.io": "2.0.3",
    "string-mask": "0.3.0",
    "url": "0.11.0"
  },
  "devDependencies": {
    "@types/body-parser": "1.16.5",
    "@types/chai": "4.1.2",
    "@types/cors": "2.8.1",
    "@types/express": "4.0.37",
    "@types/http-status": "0.2.30",
    "@types/jsonwebtoken": "7.2.3",
    "@types/mocha": "2.2.48",
    "@types/morgan": "1.7.33",
    "@types/node": "8.0.28",
    "@types/reflect-metadata": "0.0.5",
    "@types/sequelize": "4.0.79",
    "@types/socket.io": "1.4.30",
    "@types/supertest": "2.0.4",
    "chai": "4.1.2",
    "express-validator": "4.3.0",
    "install": "0.10.1",
    "mocha": "5.0.0",
    "mysql2": "1.5.0",
    "path": "0.12.7",
    "sequelize-typescript": "0.6.0",
    "supertest": "3.0.0",
    "testdouble": "3.3.3",
    "ts-node": "3.3.0",
    "typescript": "2.5.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nosbielc/PSP-backend.git"
  },
  "homepage": "https://github.com/Nosbielc/PSP-backend/blob/master/README.md"
}
